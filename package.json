{
  "name": "node_crud-api",
  "version": "1.0.0",
  "description": "simple CRUD node js api",
  "main": "dist/bundle.js",
  "scripts": {
    "start:dev": "nodemon",
    "start:prod": "npm run build && node --env-file=.env dist/bundle.js",
    "build": "webpack --mode production",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest --detectOpenHandles",
    "test:watch": "npm run test -- --watch"
  },
  "keywords": [
    "nodejs",
    "crud"
  ],
  "author": "vasily",
  "license": "ISC",
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  }
}
